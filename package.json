{
  "name": "meteomix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint:eslint": "eslint src --ext ts,tsx",
    "lint:eslint:fix": "eslint src --ext ts,tsx --fix",
    "lint:fsd": "steiger ./src",
    "lint:fsd:watch": "steiger ./src --watch",
    "lint:fsd:fix": "steiger ./src --fix",
    "lint": "npm-run-all --parallel --print-label lint:eslint lint:fsd",
    "lint:fix": "npm-run-all --parallel --print-label lint:eslint:fix lint:fsd:fix",
    "preview": "vite preview",
    "analyze": "fsd-crawler",
    "prepare": "husky"
  },
  "dependencies": {
    "@mantine/core": "^8.3.9",
    "@mantine/hooks": "^8.3.9",
    "@reduxjs/toolkit": "^2.11.0",
    "@tabler/icons-react": "^3.35.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@feature-sliced/steiger-plugin": "^0.5.7",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fsd-crawler": "^1.0.11",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "steiger": "^0.5.11",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
